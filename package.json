{
  "name": "myotobox",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "postbuild": "react-snap",
    "extract-css": "node extractCriticalCss.js",
    "serve": "http-server ./dist",
    "inline-css": "gulp",
    "update-css": "npm run build && npm run serve & npm run extract-css && npm run inline-css && pkill -f http-server"
  },
  "reactSnap": {
    "inlineCss": true,
    "source": "dist",
    "destination": "dist",
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "puppeteer": {
      "timeout": 60000
    }
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^42.0.1",
    "@ckeditor/ckeditor5-react": "^8.0.0",
    "@coreui/coreui": "^5.0.2",
    "@coreui/react": "^5.1.0",
    "@reduxjs/toolkit": "^2.2.3",
    "@tinymce/tinymce-react": "^5.0.1",
    "axios": "^1.6.1",
    "install": "^0.13.0",
    "libphonenumber-js": "^1.11.4",
    "lightbox.js-react": "^1.0.6",
    "lodash": "^4.17.21",
    "npm": "^10.8.0",
    "puppeteer": "^22.13.1",
    "react": "^18.2.0",
    "react-content-loader": "^7.0.0",
    "react-dom": "^18.2.0",
    "react-grid-gallery": "^1.0.1-alpha.0",
    "react-helmet-async": "^2.0.5",
    "react-image-grid-gallery": "^2.0.1",
    "react-otp-input": "^3.1.0",
    "react-phone-number-input": "^3.3.7",
    "react-photo-album": "^2.3.1",
    "react-quill": "^2.0.0",
    "react-redux": "^9.1.1",
    "react-router-dom": "^6.15.0",
    "react-select": "^5.8.0",
    "react-spinners": "^0.13.8",
    "react-sticky-el": "^2.1.0",
    "react-super-seo": "^1.1.9",
    "react-toastify": "^9.1.3",
    "redux-persist": "^6.0.0",
    "stickyfilljs": "^2.1.0",
    "vite-ssg": "^0.23.8",
    "yet-another-react-lightbox": "^3.17.4",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "critical": "^7.1.2",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "gulp": "^5.0.0",
    "gulp-inline-source": "^4.0.0",
    "gulp-replace": "^1.1.4",
    "html-critical-webpack-plugin": "^2.1.0",
    "penthouse": "^2.3.3",
    "react-snap": "^1.23.0",
    "vite": "^4.4.5"
  }
}
